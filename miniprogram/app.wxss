/**app.wxss**/
page{
	background-color: #eee;
  /*position: relative;*/
}
.userinfo-avatar {
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  margin: 20rpx;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
}
.mini-avatar{
  width: 100rpx;
  height: 100rpx;
}
.mask{
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  z-index:1;
  background-color:black;
  opacity:0.2;
}
.custom-class{
  z-index:99;
  margin:auto;
  position:absolute;
  left:50%;
  top:50%;
  transform:translate(-50%,-50%);
  background-color:black;
  opacity:0.5;
  border-radius:6rpx;
  height: 120rpx;
  width: 120rpx;
}
.van-loading{
  position:absolute;
  left:50%;
  top:50%;
  transform:translate(-50%,-50%);

}
.goods_pic{
  width: 150rpx;
  height: 150rpx;
  margin: 20rpx 10rpx 10rpx 20rpx;

}
.pic_box{
  display: flex;
  width: 100%;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.pic_box, .txt_box{
  position:relative;
  bottom:50rpx;
}
.item_right{
  display: inline-block;
  width: 80%;
  margin-top: 20rpx;
  margin-left:30rpx;
}
.msg-item{
  display: flex;
  flex-direction: row;
  border-bottom: 2rpx solid #eee;
  margin-top: 10rpx;
}
.nickName{
  color: #2875b8;
  font-size: 30rpx;
}
.item_title{
  font-size: 26rpx;
  margin-top: 10rpx;
  width:70%;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.g_type{
  font-size: 22rpx;
  color: #2875b8;
  margin-top: 10rpx;
}
.pub_time{
  font-size: 22rpx;
  color: gray;
}
.footer{
  color: gray;
  font-size: 20rpx;
  text-align: center;
}
.price{
  color: #e76f33;
  font-size: 40rpx;
  float: right;
  right:66rpx;
  position:relative;
  bottom:62rpx;
}
.price .tag{
  font-size: 26rpx;
}
.slide-image{
  width: 750rpx;
}
